<h1 class="text-center" style="color: white;margin-top: 50px;margin-bottom: 30px;">Chi Tiết Đơn Hàng</h1>
<div class="contrainer d-flex justify-content-center" >
    
    <table class="table table-striped table-hover table-bordered" style="background-color: white;width: 80%;margin-bottom: 400px;">
        <thead >
            <tr>
                <th>STT</th>
                <th>Mã Ghế</th>
                <th>Tên Phim</th>
                <th>Ngày Chiếu</th>
                <th>Suất Chiếu</th>
                <th>Tên Rạp</th>
            </tr>
        </thead>
        <% var i=1; 
        listorrder.forEach(function(value, index){ 
         %>
            <tr >
                <td><%= i %></td>
                <td><%= value.maghe %></td>
                <%  [value.datcho].forEach(function(item, index){ 
              %>
                <td><%= item.suatchieu.phim.ten %></td>
                <td><%= item.suatchieu.ngaychieu %></td>
                <td><%= item.suatchieu.batdau %></td>
                <td><%= item.suatchieu.rap.ten %></td>
                <% i++; 
            });
        }); %>    
            </tr>
    </table>
</div>